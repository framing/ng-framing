{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "<IMAGE_NAME>:<TAG>",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "8080"
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/app/mydb",
      "ContainerDirectory": "/opt/app"
    }
  ],
  "Logging": "/var/log/app"
}
